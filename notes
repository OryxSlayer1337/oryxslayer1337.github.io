  switch (_accountType)
            {
                case AccountType.FREE_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.FreeAccount;
                    break;

                case AccountType.DONOR_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.DonorAccount;
                    break;

                case AccountType.VETERAN_DONOR_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.VetranDonor;
                    break;

                case AccountType.SUPER_DONOR_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.SuperDonor;
                    break;

                case AccountType.EXTREME_DONOR_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.ExtremeDonor;
                    break;

                case AccountType.MODERATOR_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.ModeratorAccount;
                    break;

                case AccountType.ADMIN_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.AdminAccountr;
                    break;

                case AccountType.DEVELOPER_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.DeveloperAccount;
                    break;

                case AccountType.FOUNDER_ACCOUNT:
                    icon.Effect = ConditionEffectIndex.FounderAccount;
                    break;
            }
            -----------
            
              public int Experience(int level, int experience)
        {
            if (_accountType == AccountType.DONOR_ACCOUNT)
                return level < 20 ? (int) (experience * 1.5) : (int) (experience * 1.05);
            if (_accountType == AccountType.VETERAN_DONOR_ACCOUNT)
                return level < 20 ? (experience * 2) : (int) (experience * 1.1);
            if (_accountType == AccountType.SUPER_DONOR_ACCOUNT)
                return level < 20 ? (experience * 3) : (int)(experience * 1.7);
            if (_accountType == AccountType.EXTREME_DONOR_ACCOUNT)
                return level < 20 ? (experience * 3) : (int)(experience * 2.3);
            if (_accountType == AccountType.MODERATOR_ACCOUNT)
                return level < 20 ? (experience * 4) : (int)(experience * 2.9);
            if (_accountType == AccountType.ADMIN_ACCOUNT)
                return level < 20 ? (experience * 4) : (int)(experience * 2.9);
            if (_accountType == AccountType.DEVELOPER_ACCOUNT)
                return level < 20 ? (experience * 4) : (int)(experience * 2.9);
            if (_accountType == AccountType.FOUNDER_ACCOUNT)
                return level < 20 ? (experience * 4) : (int)(experience * 2.9);
            return experience;
        }
        ----------
        public AccountTypePerks(int accountType)
        {
            _accountType = (AccountType) accountType;
            _accessToDrastaCitadel = _accountType >= AccountType.DONOR_ACCOUNT;
            _byPassKeysRequirements = _accountType >= AccountType.VETERAN_DONOR_ACCOUNT;
            _byPassEggsRequirements = _accountType >= AccountType.SUPER_DONOR_ACCOUNT;
            _priorityToLogin = _accountType >= AccountType.EXTREME_DONOR_ACCOUNT;
        }
